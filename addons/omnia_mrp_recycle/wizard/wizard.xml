<odoo> 

	<record id="view_stock_recicle_product_wizard" model="ir.ui.view">
        <field name="name">Recicle wizard</field>
        <field name="model">stock.recicle_product</field>
        <field name="arch" type="xml">
        <form>
            <group col="4" name="From">
					<field name="from_product_id"/>
					<field name="from_qty"/>
					<field name="from_product_uom"/>
					<field name="from_location"/>
			</group>
			<group col="4" name="To">
					<field name="to_product_id"/>
					<field name="to_qty"/>
					<field name="to_product_uom"/>
					<field name="to_location"/>
            </group>
            <group>
            	    <field name="description"/>
            </group>
            <separator string=""/>
            <footer>
                <button string="Cancel"
                        class="btn-default"
                        special="cancel"/>
                <button string="Recicle"
                        name="button_recicle"
                        type="object"/>
            </footer>
        </form>
        </field>
    </record>

</odoo>
