<odoo>     

    <record model="ir.ui.view" id="view_recicle_form">
        <field name="name">stock.recicle_product.view</field>
        <field name="model">stock.recicle_product</field>
        <field name="type">form</field>
        <field name="priority">10</field>
        <field name="arch" type="xml">
            <form string="Recycle" create="0">
            	<sheet>	
            		 <button string="Moves"
                        name="button_open_moves"
                        type="object"/>
            		<group >
            			<field name="name"/>
            		</group>
		            <group col="4" name="From">
							<field name="from_product_id"/>
							<field name="from_qty"/>
							<field name="from_product_uom"/>
							<field name="from_location"/>
					</group>
					<group col="4" name="To">
							<field name="to_product_id"/>
							<field name="to_qty"/>
							<field name="to_product_uom"/>
							<field name="to_location"/>
		            </group>
		            <group>
		            	    <field name="description"/>
		            </group>
               	</sheet> 
            </form>
        </field>
    </record>
    
    <record model="ir.ui.view" id="view_recicle_tree">
        <field name="name">stock.recicle_product.tree</field>
        <field name="model">stock.recicle_product</field>
        <field name="type">tree</field>
        <field name="priority" eval="1"/>
        <field name="arch" type="xml">
            <tree string="Recycle" create="0">
                   <field name="name"/>
                   <field name="description"/>
                   <field name="from_product_id"/>
                   <field name="from_location"/>
                   <field name="to_product_id"/>
                   <field name="to_location"/>
            </tree>
        </field>
    </record>

    <record id="action_mrp_recicle_wizard" model="ir.actions.act_window">
        <field name="name">Recycle</field>
        <field name="res_model">stock.recicle_product</field>
        <field name="view_type">form</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="view_id" ref="omnia_mrp_recycle.view_stock_recicle_product_wizard"/>
    </record>

    <record id="action_mrp_recicle_moves" model="ir.actions.act_window">
        <field name="name">Recycle Moves</field>
        <field name="res_model">stock.recicle_product</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
    
    <menuitem action="action_mrp_recicle_wizard" id="menu_mrp_recicle_wizard" parent="stock.menu_stock_warehouse_mgmt" sequence="136"/>
    <menuitem action="action_mrp_recicle_moves" id="menu_mrp_recicle_moves" parent="stock.menu_stock_warehouse_mgmt" sequence="137"/>
    
</odoo>
