<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="dichiarazione_intento_view_partner_property_form" model="ir.ui.view">
			<field name="name">res.partner.property.inherit.dichiarazione.intento.form</field>
			<field name="model">res.partner</field>
			<field name="priority">2</field>
			<field name="inherit_id" ref="account.view_partner_property_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='bank_ids']" position="after">
					<field name="dichiarazione_intento">
						<tree string="Bank Details">
							<field name="name"/>
							<field name="date_start"/>
							<field name="date_end"/>
							<field name="state"/>
                        </tree>
					</field>
				</xpath>
			</field>
		</record>
	</data>
</openerp>